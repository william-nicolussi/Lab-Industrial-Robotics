 RowBox[{
  RowBox[{
   RowBox[{"Cap", "=", 
    RowBox[{"1.", "*", 
     RowBox[{"10", "^", 
      RowBox[{"-", "6"}]}]}]}], ";"}], 
  RowBox[{"(*", "farad", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"tau", "=", 
   RowBox[{"R", "*", "Cap"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"Vin", "[", "t_", "]"}], ":=", 
   RowBox[{"5", " ", 
