     RowBox[{"Vc", "[", "t", "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", "0", ",", 
       RowBox[{"5", "*", "tau"}]}], "}"}]}], "]"}]}], ";"}], "\n", 
